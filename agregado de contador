int posicion = 0;
                        	for(int contador=1; contador<cliente.Lenght; contador++)
                            {
                                {
                        		if (cliente[posicion] > cliente[contador])
                                {
                        				posicion=contador;
                                        }
                                        }
                                        return posicion;
                                        }





